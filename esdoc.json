{
  "source": "./src",
  "destination": "./doc",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "upload-gphotos | ESDoc"
        }
      }
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [{
          "from": "^src/",
          "to": "lib/"
        }]
      }
    }
  ]
}
