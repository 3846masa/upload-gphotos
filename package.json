{
  "name": "upload-gphotos",
  "version": "0.1.0",
  "description": "Upload to Google Photos",
  "main": "lib/index.js",
  "bin": {
    "upload-gphotos": "lib/cli.js"
  },
  "nexe": {
    "input": "./lib/cli.js",
    "output": "upload-gphotos^$",
    "nodeMakeArgs": [
      "-j",
      "4"
    ],
    "browserify": {
      "requires": [],
      "excludes": [
        "cls-bluebird",
        "canvas"
      ]
    },
    "runtime": {
      "framework": "node",
      "version": "4.4.7",
      "ignoreFlags": false
    }
  },
  "dependencies": {
    "co": "4.6.0",
    "colors": "1.1.2",
    "fs-promise": "0.5.0",
    "jsdom": "9.4.1",
    "log4js": "0.6.38",
    "progbar": "1.1.0",
    "progress": "1.1.8",
    "read": "1.0.7",
    "request": "2.72.0",
    "request-promise": "3.0.0",
    "tough-cookie": "2.2.2",
    "yargs": "4.8.1"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-async-to-module-method": "6.8.0",
    "babel-plugin-transform-class-properties": "6.9.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.8.0",
    "babel-preset-es2015-node4": "2.1.0",
    "esdoc": "0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-importpath-plugin": "0.0.1",
    "eslint": "2.11.1",
    "nexe": "1.1.2",
    "npm-run-all": "2.3.0",
    "sane": "1.3.4",
    "source-map-support": "0.4.0"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:code build:doc",
    "build:code": "npm-run-all build:babel build:nexe",
    "build:babel": "babel src --out-dir lib",
    "build:doc": "esdoc -c ./esdoc.json",
    "build:nexe": "nexe",
    "watch": "sane \"npm-run-all --parallel build:babel build:doc\" src --glob='**/*.js'",
    "watch:babel": "npm run build:babel --source-maps",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "3846masa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3846masa/upload-gphotos.git"
  },
  "bugs": {
    "url": "https://github.com/3846masa/upload-gphotos/issues"
  },
  "homepage": "https://github.com/3846masa/upload-gphotos#readme"
}
